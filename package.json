{
	"name": "package-json-helper",
	"displayName": "package-json-helper",
	"description": "a package.json dependancy helper,goto definition quickly and show details in hover",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "package-json-helper.helloWorld",
				"title": "Hello World"
			}
    ],
    "colors": [
      {
        "id": "packagejson.lineForegroundColor",
        "description": "Specifies the foreground color of the blame annotation for the current line",
        "defaults": {
          "dark": "#ffffffaa",
          "light": "#837f7f50",
          "highContrast": "#837f7f50"
        }
      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/shelljs": "^0.8.8",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"shelljs": "^0.8.4"
	}
}
